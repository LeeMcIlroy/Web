@{
    ViewBag.Title = "insights";
}

@{ var meta_sitesec = "GCLabs"; }
@{ var meta_sitename = "GC녹십자의료재단"; }
@{ var meta_title = "Academic Insights"; }
@{ var meta_Site_image = "http://121.140.143.35:4060/assets/images/gclabs-logo.png"; }
@{ var meta_description = "Provision of curriculum which helps to maintain high quality even after lab construction"; }
@{ var meta_page_url = "http://121.140.143.35:4060/eng/business"; }
@{ var meta_keywords = "BUSINESS, Academic Insights"; }
@{ var meta_twitter_site = ""; }


@section meta {
    <title>@meta_title</title>
    <meta name="description" content="@meta_description" />
    <meta name="keywords" content="@meta_keywords" />
    <meta property="og:title" content="@meta_title" />
    <meta property="og:type" content="웹 사이트" />
    <meta property="og:description" content="@meta_description" />
    <meta property="og:image" content="@meta_Site_image" />
    <meta property="og:url" content="@meta_page_url" />
    <meta property="og:site_name" content="@meta_sitename" />
    <meta name="twitter:card" content="요약" />
    <meta name="twitter:site" content="@meta_twitter_site" />
    <meta name="twitter:title" content="@meta_title" />
    <meta name="twitter:description" content="@meta_description" />
    <meta name="ms.sitename" content="@meta_sitename" />
    <meta name="ms.sitesec" content="@meta_sitesec" />
}
@section script {

    <script type="text/javascript" src="../../assets/lib/jquery-2.2.4.min.js"></script>
    <!-- Live List Layout -->
    <script type="text/javascript" src="../../assets/lib/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="../../assets/lib/slick.min.js"></script>
    <script type="text/javascript" src="../../assets/js/app.js"></script>


}
<div id="container" class="container gclabs">
    <div class="contents-wrap">
        <div class="contents-inner">

            <div class="breadcrumb">
                <span><a href="#">Home</a></span>


                <span><a href="#">BUSINESS</a></span>
                <!-- <span><a href="#">Business Areas</a></span> -->

                <span class="current">Academic Insights</span>
            </div>

            <div class="page-title">
                <h2 class="title">Academic Insights</h2>
                <p class="text">Provision of curriculum which helps to maintain high quality even after lab construction</p>
            </div>



            <div class="board-wrap">
                <div class="info-box">
                    <p class="text3">
                        Provision of curriculum (staff training, conference, etc.) which helps to maintain high quality even after lab construction

                    </p>
                </div>
                <div class="board-list">
                    <div class="list-wrap">
                        <ul class="lists type-row biological">
                            <li class="item">
                                <div class="box">
                                    <div class="thumbnail">
                                        <img src="../../../assets/images/img-insi-01.png" alt="">
                                    </div>
                                    <div class="contents">
                                        <strong class="title">Training</strong>
                                        <p class="content">Invite local employees to GC Labs for training</p>
                                        <div class="content">
                                            <ul class="list-style dot">
                                                <li>Education of GC Lab’s state-of-the-art facility</li>
                                                <li>Education of Lab Information System (LIS)</li>
                                                <li>Education of test item</li>

                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            <li class="item">
                                <div class="box">
                                    <div class="thumbnail">
                                        <img src="../../../assets/images/img-insi-02.png" alt="">
                                    </div>
                                    <div class="contents">
                                        <strong class="title">Conference / Seminar</strong>
                                        <p class="content">Conference & seminar support for partners</p>
                                        <div class="content">
                                            <ul class="list-style dot">
                                                <li>Co-arrange academic seminar on either a regular or occasional basis to show lab facility’s superiority and provide marketing material</li>
                                                <li>Localize GC Labs’ marketing material & provide translated version to local labs</li>

                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>

