@{
    ViewBag.Title = "how";
}

@{ var meta_sitesec = "GCLabs"; }
@{ var meta_sitename = "GC녹십자의료재단"; }
@{ var meta_title = "How to Order Tests"; }
@{ var meta_Site_image = "http://121.140.143.35:4060/assets/images/gclabs-logo.png"; }
@{ var meta_description = "Please consult our How to Order Tests and Test Information sections."; }
@{ var meta_page_url = "http://121.140.143.35:4060/eng/test"; }
@{ var meta_keywords = "test, How to Order Tests"; }
@{ var meta_twitter_site = ""; }


@section meta {
    <title>@meta_title</title>
    <meta name="description" content="@meta_description" />
    <meta name="keywords" content="@meta_keywords" />
    <meta property="og:title" content="@meta_title" />
    <meta property="og:type" content="웹 사이트" />
    <meta property="og:description" content="@meta_description" />
    <meta property="og:image" content="@meta_Site_image" />
    <meta property="og:url" content="@meta_page_url" />
    <meta property="og:site_name" content="@meta_sitename" />
    <meta name="twitter:card" content="요약" />
    <meta name="twitter:site" content="@meta_twitter_site" />
    <meta name="twitter:title" content="@meta_title" />
    <meta name="twitter:description" content="@meta_description" />
    <meta name="ms.sitename" content="@meta_sitename" />
    <meta name="ms.sitesec" content="@meta_sitesec" />
}
@section script {

    <script type="text/javascript" src="../../assets/lib/jquery-2.2.4.min.js"></script>
    <!-- Live List Layout -->
    <script type="text/javascript" src="../../assets/lib/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="../../assets/lib/slick.min.js"></script>
    <script type="text/javascript" src="../../assets/js/app.js"></script>


}


<div id="container" class="container dept">
    <div class="contents-wrap">
        <div class="contents-inner">

            <div class="breadcrumb">
                <span><a href="#">Home</a></span>


                <span><a href="#">test</a></span>


                <span class="current">How to Order Tests</span>
            </div>

            <div class="page-title">
                <h2 class="title">How to Order Tests</h2>
                <p class="text">Please consult our "How to Order Tests" and "Test Information" sections.</p>
            </div>

            <div class="banner-wrap">
                <div class="banner-inner">
                    <div class="image-area">
                        <img src="../../../assets/images/img-lm-hw-01-pc.png" class="pc tablet" alt="">
                        <img src="../../../assets/images/img-lm-hw-01-mo.png" class="mobile" alt="">
                    </div>
                    <div class="text-area">
                        <span class="title">Sample transportation</span>
                        <p class="desc">When outsourcing test to GC Labs, it is important to understand the work flow and the amount <br class="mobile">of the time between each processes.</p>
                    </div>
                </div>
            </div>

            <div class="ethics-wrap">
                <div class="contents-section">
                    <div class="title-wrap">
                        <h3 class="title-h3">Steps are performed in this order</h3>

                    </div>
                </div>
                <div class="global-img-area">
                    <div class="img-area">

                        <img src="../../../assets/images/img-lm-hw-02-pc.png" class="pc tablet" alt="">
                        <img src="../../../assets/images/img-lm-hw-02-mo.png" class="mobile" alt="">

                    </div>
                </div>
                <p class="bottom-desc">All three temperatures (ambient, refrigerated, frozen) can be sent but please pre-inform GC Labs whenever it is not a transportation on a regular basis. In prior to sending the specimen, please proceed the requisition first through GC Labs website (www.gclabs.co.kr/eng) </p>
            </div>
            <div class="test-button-wrap">
                <a href="/eng/qna/write" class="btn btn-arrow layer"><span>Contact Us</span></a>
            </div>
        </div>



    </div>
</div>

