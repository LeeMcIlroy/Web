@{
    ViewBag.Title = "mission";
}

@{ var meta_sitesec = "GCLabs"; }
@{ var meta_sitename = "GC녹십자의료재단"; }
@{ var meta_title = "Mission & Vision"; }
@{ var meta_Site_image = "http://121.140.143.35:4060/assets/images/gclabs-logo.png"; }
@{ var meta_description = "The philosophy at GC Labs for a brighter and healthier tomorrow"; }
@{ var meta_page_url = "http://121.140.143.35:4060/eng/about"; }
@{ var meta_keywords = "ABOUT, Mission & Vision"; }
@{ var meta_twitter_site = ""; }


@section meta {
    <title>@meta_title</title>
    <meta name="description" content="@meta_description" />
    <meta name="keywords" content="@meta_keywords" />
    <meta property="og:title" content="@meta_title" />
    <meta property="og:type" content="웹 사이트" />
    <meta property="og:description" content="@meta_description" />
    <meta property="og:image" content="@meta_Site_image" />
    <meta property="og:url" content="@meta_page_url" />
    <meta property="og:site_name" content="@meta_sitename" />
    <meta name="twitter:card" content="요약" />
    <meta name="twitter:site" content="@meta_twitter_site" />
    <meta name="twitter:title" content="@meta_title" />
    <meta name="twitter:description" content="@meta_description" />
    <meta name="ms.sitename" content="@meta_sitename" />
    <meta name="ms.sitesec" content="@meta_sitesec" />
}
@section script {

    <script type="text/javascript" src="../../assets/lib/jquery-2.2.4.min.js"></script>
    <!-- Live List Layout -->
    <script type="text/javascript" src="../../assets/lib/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="../../assets/lib/slick.min.js"></script>
    <script type="text/javascript" src="../../assets/js/app.js"></script>


}
<div id="container" class="container gclabs">
    <div class="contents-wrap">
        <div class="contents-inner">

            <div class="breadcrumb">
                <span><a href="#">Home</a></span>


                <span><a href="#">ABOUT</a></span>


                <span class="current">Mission &amp; Vision</span>
            </div>

            <div class="page-title">
                <h2 class="title">Mission &amp; Vision</h2>
                <p class="text">The philosophy at GC Labs for a brighter and healthier tomorrow</p>
            </div>

            <div class="vision-wrap">
                <div class="top-info">
                    <strong>GC Labs</strong>
                    <p class="text">contributes to the enhancement of the nation's health and wellness preservation and to the advancement <br>of laboratory medicine through its medical services.</p>
                    <div class="img-area">

                        <img src="../../../assets/images/img-mission-en.png" class="pc tablet" alt="Green Cross Laboratories(GC Labs)">
                        <img src="../../../assets/images/img-mission-mo-en.png" class="mobile" alt="Green Cross Laboratories(GC Labs)">


                    </div>

                </div>



                <div class="section">

                    <div class="title-wrap">




                        <h3 class="title-h3">GC Labs Mission</h3>






                    </div>
                    <div class="gc-text-box mission-area">
                        <div class="img-area">

                            <img src="../../../assets/images/img-gc-mission-pc.png" class="pc tablet" alt="">
                            <img src="../../../assets/images/img-gc-mission-mo.png" class="mobile" alt="">


                        </div>
                        <div class="text-area">
                            <div class="inner">
                                <strong>Contributing to Healthy Life of Humankind</strong>
                                <!--<p class="text">
                                     We provide clients with world-class clinical laboratory testing service and value, and our staff with pride and self-esteem. We will become a responsible laboratory to make a healthy society through continuous changes and innovations..
                                </p>-->
                            </div>
                        </div>
                    </div>
                </div>



                <div class="section">

                    <div class="title-wrap">




                        <h3 class="title-h3">GC Labs Vision</h3>






                    </div>
                    <div class="gc-text-box vision-area">
                        <div class="img-area">

                            <img src="../../../assets/images/img-gc-vision-pc.png" class="pc tablet" alt="">
                            <img src="../../../assets/images/img-gc-vision-mo.png" class="mobile" alt="">


                        </div>
                        <div class="text-area">
                            <div class="inner">
                                <strong>Growing with Our Clients to a World-class Laboratories</strong>
