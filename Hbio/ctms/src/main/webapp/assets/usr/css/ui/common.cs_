.tab-box .tab-btn {margin-bottom:0px; position:relative;}
.tab-box .tab-btn:before {content:''; display:block; position:absolute; bottom:0; left:0; width:100%; height:1px; background-color:#999; }

.tab-box  #tab1,
.tab-box  #tab2,
.tab-box  #tab3,
.tab-box  #tab4,
.tab-box  #tab5,
.tab-box  #tab6 {display:none;}


.tab-box #tab1:checked ~ .tab-btn label.tab1,
.tab-box #tab2:checked ~ .tab-btn label.tab2,
.tab-box #tab3:checked ~ .tab-btn label.tab3,
