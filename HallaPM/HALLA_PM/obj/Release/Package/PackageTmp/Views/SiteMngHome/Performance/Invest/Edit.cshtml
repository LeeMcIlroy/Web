@using HALLA_PM.Models;
@using HALLA_PM.Util;
@using HALLA_PM.Core;
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutSiteMng.cshtml";

    PmInvest pmInvest = Model.pmInvest;
    OrgCompany orgCompany = Model.orgCompany;
    List<OrgBusiness> orgBusiness = Model.orgBusiness;

    string excel = Model.excel;

    // 전년도 연간 누계  -  12월 정보의 누계
    List<PmInvestBusiness> prevPmInvestBusinessByYearly = Model.prevPmInvestBusinessByYearly;

    // 전년도 당월 및 누계
    List<PmInvestBusiness> prevPmInvestBusinessByMonthly = Model.prevPmInvestBusinessByMonthly;

    // 당해년도 연간 계획 정보
    List<PlanYearInvestBusiness> planYearInvestBusiness = Model.planYearInvestBusiness;

    // 당해년도 월별 계획 정보
    List<PlanMonthlyInvestBusiness> planMonthlyInvestBusiness = Model.planMonthlyInvestBusiness;

    // 당해년도 월별 누적계획 정보
    List<PlanMonthlyInvestBusiness> planMonthlyInvestBusinessSummary = Model.planMonthlyInvestBusinessSummary;

    // excel 업로드 데이터
    List<PmInvestBusiness> uploadedPmInvestBusiness = Model.uploadedPmInvestBusiness;

    // 저장 데이터
    List<PmInvestBusiness> currentPmInvestBusiness = Model.currentPmInvestBusiness;

    // 하단 투자/인원 요약 부 데이터
    PmInvestSum prevYearlyInfo = Model.prevYearlyInfo;
    PmInvestSum prevMonthlyInfo = Model.prevMonthlyInfo;
    PmInvestSum prevMonthlyInfoSummary = Model.prevMonthlyInfoSummary;

    PlanYearInvestSummary planYearInvestSummary = Model.planYearInvestSummary;
    PlanMonthlyInvestSummaryExp currentYearlyPlanInfo = Model.currentYearlyPlanInfo;
    PlanMonthlyInvestSummaryExp currentMonthlyPlanInfo = Model.currentMonthlyPlanInfo;

    List<PmInvestSum> currentPmInvestSum = Model.currentPmInvestSum;


    // 관리자 댓글 등록
    CommentList commentList = new CommentList();
    commentList.CommentYear = pmInvest.InvestYear;
    commentList.CommentMonth = pmInvest.Monthly;
    commentList.AttachTableName = "COMPANY";
    commentList.AttachTableSeq = pmInvest.OrgCompanySeq;
    commentList.UserKey = SessionManager.GetAdminSession().insaUserV.userKey;
    commentList.EmpNo = SessionManager.GetAdminSession().insaUserV.empNo;
    commentList.UserKorName = SessionManager.GetAdminSession().insaUserV.userKorName;
    commentList.CompKorName = SessionManager.GetAdminSession().insaDeptV == null ? "소속검색안됨" : SessionManager.GetAdminSession().insaDeptV.Compkorname;
    commentList.DeptKorName = SessionManager.GetAdminSession().insaDeptV == null ? "소속검색안됨" : SessionManager.GetAdminSession().insaDeptV.Deptkorname;
}

<div id="container">
    <div class="location">
        <span class="home"><i class="fa fa-home"></i></span>
        <span>경영실적</span>
        <strong>투자 &middot; 인원</strong>
    </div>

    <form name="editForm" id="editForm" method="post" enctype="multipart/form-data">
        <input type="hidden" name="Seq" id="Seq" value="@pmInvest.Seq" />
        <input type="hidden" name="PmInvestSeq" id="PmInvestSeq" value="@pmInvest.Seq" />
        <input type="hidden" name="OrgCompanySeq" id="OrgCompanySeq" value="@orgCompany.Seq" />
        <input type="hidden" name="RegistStatus" id="RegistStatus" value="@pmInvest.RegistStatus" />
        <input type="hidden" name="isEdit" id="isEdit" value="Y" />
        <div class="con_box search">
            <h3 class="mt0">투자 &middot; 인원 작성</h3>
            <div class="bg_gray">
                <i class="fa fa-info-circle" aria-hidden="true"></i> 계산하기를 클릭하시면 비율, 합계 등을 미리 확인하실 수 있습니다.<br />
                <i class="fa fa-info-circle" aria-hidden="true"></i> 승인요청, 승인 클릭 시 수정이 불가능하니 신중히 작성해주세요.
            </div>
        </div>
        <!--// con_box -->

        <div class="con_box">
            <table class="tbl th_ver ta_left">
                <caption></caption>
                <colgroup>
                    <col width="15%" />
                    <col width="/" />
                </colgroup>
                <tbody>
                    <tr>
                        <th scope="row">등록연월</th>
                        <td>@pmInvest.InvestYear 년 &nbsp;&nbsp; @pmInvest.Monthly 월</td>
                    </tr>
                    <tr>
                        <th scope="row">회사</th>
                        <td class="align_left_right mt0">
                            <table class="tbl2">
                                <tr>
                                    <td>@orgCompany.UnionName > @orgCompany.CompanyName</td>
                                        @if (pmInvest.RegistStatus == Define.REGIST_STATUS.GetKey("저장")
                                            || pmInvest.RegistStatus == Define.REGIST_STATUS.GetKey("1차반려")
                                            || pmInvest.RegistStatus == Define.REGIST_STATUS.GetKey("2차반려"))
                                        {
                                            <td class="ta_right">
                                                <a href="javascript: onConfirm(document.editForm);" class="btn small full_green right">승인요청</a>
                                            </td>
                                        }
                                        else if (pmInvest.RegistStatus == Define.REGIST_STATUS.GetKey("승인대기")
                                            || pmInvest.RegistStatus == Define.REGIST_STATUS.GetKey("1차승인"))
                                        {
                                            <td class="ta_right">
                                                <button class="btn small full_green" type="button" onclick="onRegistReply();">댓글등록</button>
                                                <a href="javascript: onReject(document.editForm);" class="btn small full_gray right">반려</a>
                                                <a href="javascript: onConfirm(document.editForm);" class="btn small full_green right">승인</a>
                                            </td>
                                        }
                                        else if (pmInvest.RegistStatus == Define.REGIST_STATUS.GetKey("최종승인"))
                                        {
                                            <td class="ta_right">
                                                <button class="btn small full_green" type="button" onclick="onRegistReply();">댓글등록</button>
                                            </td>
                                        }
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <th scope="row">업로드</th>
                        <td class="align_left_right mt0">
                            <div class="style_file" style="width:60%;">
                                <label>
                                    <span></span>
                                    <input type="file" name="ExcelFile" id="ExcelFile">
                                    <button type="button" class="btn full_gray">파일찾기</button>
                                </label>
                            </div>
                            <a href="javascript: uploadExcel(document.editForm);" class="btn small full_gray">엑셀 반영</a>
                            <a href="javascript: onExcelDown(document.editForm);" class="btn small full_gray right">양식다운로드</a>
                        </td>
                    </tr>
                </tbody>
            </table>
            <!--// tbl th_ver -->

            @if (currentPmInvestBusiness != null)
            {

                <h3>[@pmInvest.Monthly]월 투자,인원 요약</h3>
                <table class="tbl th_hor" border="0" cellpadding="0" cellspacing="0">
                    <caption></caption>
                    <colgroup>
                        <col width="7%" />
                        <col width="7%" />
                        <col width="7%" />
                        <col width="/" />
                    </colgroup>
                    <thead>
                        <tr>
                            <th scope="col" colspan="3"></th>
                            <th scope="col">투자</th>
                            <th scope="col">인원</th>
                            <th scope="col">국내인원</th>
                            <th scope="col">해외인원</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row" rowspan="3">@(Convert.ToInt32(pmInvest.InvestYear) - 1)년</th>
                            <th scope="row" colspan="2">연간</th>
                            <td>@WebUtil.NCommaRound(prevYearlyInfo.Investment, 0)</td>
                            <td>@WebUtil.NCommaRound(prevYearlyInfo.Personnel, 0)</td>
                            <td>@WebUtil.NCommaRound(prevYearlyInfo.DomesticPersonnel, 0)</td>
                            <td>@WebUtil.NCommaRound(prevYearlyInfo.OverseasPersonnel, 0)</td>
                        </tr>
                        <tr>
                            <th scope="row" rowspan="2">[@pmInvest.Monthly]월</th>
                            <th scope="row">당월</th>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfo.Investment, 0)</td>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfo.Personnel, 0)</td>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfo.DomesticPersonnel, 0)</td>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfo.OverseasPersonnel, 0)</td>
                        </tr>
                        <tr>
                            <th scope="row">누계</th>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfoSummary.Investment, 0)</td>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfoSummary.Personnel, 0)</td>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfoSummary.DomesticPersonnel, 0)</td>
                            <td>@WebUtil.NCommaRound(prevMonthlyInfoSummary.OverseasPersonnel, 0)</td>
                        </tr>
                        <tr>
                            <th scope="row" rowspan="9">@(pmInvest.InvestYear)년</th>
                            <th scope="row">연간</th>
                            <th scope="row">계획</th>
                            <td>@WebUtil.NCommaRound(planYearInvestSummary.Investment, 0)</td>
                            <td>@WebUtil.NCommaRound(planYearInvestSummary.Personnel, 0)</td>
                            <td>@WebUtil.NCommaRound(planYearInvestSummary.DomesticPersonnel, 0)</td>
                            <td>@WebUtil.NCommaRound(planYearInvestSummary.OverseasPersonnel, 0)</td>
                        </tr>
                        <tr>
                            <th scope="row" rowspan="4">[@pmInvest.Monthly]월</th>
                            <th scope="row">계획</th>
                            <td>@WebUtil.NCommaRound(currentMonthlyPlanInfo.Investment, 0)</td>
                            <td>@WebUtil.NCommaRound(currentMonthlyPlanInfo.Personnel, 0)</td>
                            <td>@WebUtil.NCommaRound(currentMonthlyPlanInfo.DomesticPersonnel, 0)</td>
                            <td>@WebUtil.NCommaRound(currentMonthlyPlanInfo.OverseasPersonnel, 0)</td>
                        </tr>
                        <tr>
                            <th scope="row">실적</th>
                            <td>@(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? "0" : WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().Investment, 0))</td>
                            <td>@(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? "0" : WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().Personnel, 0))</td>
                            <td>@(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? "0" : WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().DomesticPersonnel, 0))</td>
                            <td>@(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? "0" : WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().OverseasPersonnel, 0))</td>
                        </tr>
                        @{
                            decimal pmMonthlyInvestment = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().Investment;
                            decimal pmMonthlyPersonnel = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().Personnel;
                            decimal pmMonthlyDomesticPersonnel = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().DomesticPersonnel;
                            decimal pmMonthlyOverseasPersonnel = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("당월")).FirstOrDefault().OverseasPersonnel;
                        }
                        <tr>
                            <th scope="row">계획대비</th>
                            <td class="bg_blue">
                                @if (pmMonthlyInvestment == 0 || currentMonthlyPlanInfo.Investment == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyInvestment / currentMonthlyPlanInfo.Investment) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">

                                @if (pmMonthlyPersonnel == 0 || currentMonthlyPlanInfo.Personnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyPersonnel / currentMonthlyPlanInfo.Personnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">

                                @if (pmMonthlyDomesticPersonnel == 0 || currentMonthlyPlanInfo.DomesticPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyDomesticPersonnel / currentMonthlyPlanInfo.DomesticPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">

                                @if (pmMonthlyOverseasPersonnel == 0 || currentMonthlyPlanInfo.OverseasPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyOverseasPersonnel / currentMonthlyPlanInfo.OverseasPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">전년대비</th>
                            <td class="bg_blue">
                                @if (pmMonthlyInvestment == 0 || prevMonthlyInfo.Investment == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyInvestment / prevMonthlyInfo.Investment) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">

                                @if (pmMonthlyPersonnel == 0 || prevMonthlyInfo.Personnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyPersonnel / prevMonthlyInfo.Personnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">

                                @if (pmMonthlyDomesticPersonnel == 0 || prevMonthlyInfo.DomesticPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyDomesticPersonnel / prevMonthlyInfo.DomesticPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">

                                @if (pmMonthlyOverseasPersonnel == 0 || prevMonthlyInfo.OverseasPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmMonthlyOverseasPersonnel / prevMonthlyInfo.OverseasPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                        </tr>
                        <tr>
                            <th scope="row" rowspan="4">누계</th>
                            <th scope="row">계획</th>
                            <td>@WebUtil.NCommaRound(currentYearlyPlanInfo.Investment, 0)</td>
                            <td>@WebUtil.NCommaRound(currentYearlyPlanInfo.Personnel, 0)</td>
                            <td>@WebUtil.NCommaRound(currentYearlyPlanInfo.DomesticPersonnel, 0)</td>
                            <td>@WebUtil.NCommaRound(currentYearlyPlanInfo.OverseasPersonnel, 0)</td>
                        </tr>
                        <tr>
                            <th scope="row">실적</th>
                            <td>@(WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().Investment, 0))</td>
                            <td>@(WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().Personnel, 0))</td>
                            <td>@(WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().DomesticPersonnel, 0))</td>
                            <td>@(WebUtil.NCommaRound(currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().OverseasPersonnel, 0))</td>
                        </tr>
                        @{
                            decimal pmYearlyInvestment = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().Investment;
                            decimal pmYearlyPersonnel = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().Personnel;
                            decimal pmYearlyDomesticPersonnel = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().DomesticPersonnel;
                            decimal pmYearlyOverseasPersonnel = currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault() == null ? 0 : currentPmInvestSum.Where(x => x.MonthlyType == Define.MONTHLY_TYPE.GetKey("누계")).FirstOrDefault().OverseasPersonnel;
                        }
                        <tr>
                            <th scope="row">계획대비</th>
                            <td class="bg_blue">
                                @if (pmYearlyInvestment == 0 || currentYearlyPlanInfo.Investment == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyInvestment / currentYearlyPlanInfo.Investment) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">
                                @if (pmYearlyPersonnel == 0 || currentYearlyPlanInfo.Personnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyPersonnel / currentYearlyPlanInfo.Personnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">
                                @if (pmYearlyDomesticPersonnel == 0 || currentYearlyPlanInfo.DomesticPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyDomesticPersonnel / currentYearlyPlanInfo.DomesticPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">
                                @if (pmYearlyOverseasPersonnel == 0 || currentYearlyPlanInfo.OverseasPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyOverseasPersonnel / currentYearlyPlanInfo.OverseasPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">전년대비</th>
                            <td class="bg_blue">
                                @if (pmYearlyInvestment == 0 || prevMonthlyInfoSummary.Investment == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyInvestment / prevMonthlyInfoSummary.Investment) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">
                                @if (pmYearlyPersonnel == 0 || prevMonthlyInfoSummary.Personnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyPersonnel / prevMonthlyInfoSummary.Personnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">
                                @if (pmYearlyDomesticPersonnel == 0 || prevMonthlyInfoSummary.DomesticPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyDomesticPersonnel / prevMonthlyInfoSummary.DomesticPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                            <td class="bg_blue">
                                @if (pmYearlyOverseasPersonnel == 0 || prevMonthlyInfoSummary.OverseasPersonnel == 0)
                                {
                                    <p>0 %</p>
                                }
                                else
                                {
                                    <p>@WebUtil.NCommaRound(Math.Round((pmYearlyOverseasPersonnel / prevMonthlyInfoSummary.OverseasPersonnel) * 100, 2, MidpointRounding.AwayFromZero), 0) %</p>
                                }
                            </td>
                        </tr>
                    </tbody>
                </table>

            }
            
            <br />
            <hr />

            @{
                int idx = 0;
            }
            @foreach (var item in orgBusiness)
            {
                <h3>@item.BusinessName</h3>
                <table class="tbl th_hor" border="0" cellpadding="0" cellspacing="0">
                    <caption></caption>
                    <colgroup>
                        <col width="7%" />
                        <col width="7%" />
                        <col width="7%" />
                        <col width="/" />
                    </colgroup>
                    <thead>
                        <tr>
                            <th scope="col" colspan="3"></th>
                            <th scope="col">투자</th>
                            <th scope="col">인원</th>
                            <th scope="col">국내인원</th>
