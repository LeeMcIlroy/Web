                                <td>0</td>
                                <td>0</td>
                                <td>0%</td>
                                <td>0</td>
                                <td>0</td>
                            </tr>
                            <tr>
                                <th scope="row">4분기</th>
                                <td>0</td>
                                <td>0</td>
                                <td>0%</td>
                                <td>0</td>
                                <td>0</td>
                            </tr>
                    }
                    else
                    {
                        var count = 0;
                        foreach (var QuarterResultLastSummary in QuarterResultLastSummaryYearList)
                        {
                            if (count == 0)
                            {
                                <tr>
                                    <th scope="row" rowspan="@(QuarterResultLastSummaryYearList.Count() > 4 ? 4 : QuarterResultLastSummaryYearList.Count())">@(Convert.ToInt32(pmQuarterPal.QuarterPalYear) - 1)년 실적</th>
                                    <th scope="row">@(QuarterResultLastSummary.BusinessQuarter)분기</th>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.Sales, 2)</td>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.Ebit, 2)</td>
                                    <td>@WebUtil.NCommaRound(WebUtil.NumberRound(QuarterResultLastSummary.Ebit, QuarterResultLastSummary.Sales, 100, 2), 2)%</td>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.Pbt, 2)</td>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.NetProfitTerm, 2)</td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <th scope="row">@(QuarterResultLastSummary.BusinessQuarter)분기</th>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.Sales, 2)</td>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.Ebit, 2)</td>
                                    <td>@WebUtil.NCommaRound(WebUtil.NumberRound(QuarterResultLastSummary.Ebit, QuarterResultLastSummary.Sales, 100, 2), 2)%</td>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.Pbt, 2)</td>
                                    <td>@WebUtil.NCommaRound(QuarterResultLastSummary.NetProfitTerm, 2)</td>
                                </tr>
                            }
                            count++;
                        }
                    }

